    "tokenColors": [
        {
            "scope": [
                "emphasis"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "strong"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "header"
            ],
            "settings": {
                "foreground": "#83769c"
            }
        },
        {
            "scope": [
                "source"
            ],
            "settings": {
                "foreground": "#5f574f"
            }
        },
        {
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#008751"
            }
        },
        {
            "scope": [
                "markup.inserted"
